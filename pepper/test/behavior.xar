    def onInput_onStart(self, data):
        #self.onStopped() #activate the output of the box
        motion_service  = ALProxy("ALMotion")

        motion_service.setStiffnesses("RShoulderRoll", 1.0)

        # Example showing how to set angles, using a fraction of max speed
        names  = ["RShoulderRoll", "RShoulderPitch"]
        angles  = [data[0], data[1]]

        fractionMaxSpeed  = 1.0
        motion_service.setAngles(names, angles, fractionMaxSpeed)

        time.sleep(3.0)
        motion_service.setStiffnesses("RShoulderRoll", 0.0)

        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
