                                    <![CDATA[import thread, time, json, urllib, urllib2, sys, os, threading, base64

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.freez = False
        self.connectionToken = None
        self.framemanager = ALProxy("ALFrameManager")
        self.ws = None
        self.ws_connection_retry = 0
        pass

    def onUnload(self):
        self.freez = False
        self.connectionToken = None
        self.framemanager = None
        self.ws = None
        self.ws_connection_retry = 0
        pass

    def onInput_onStart(self, url):
        self.init_read_library()
        self.init_websocket_connection()
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_inputCompleteAction(self):
        self.freez = False
        self.logger.info("ACTION_COMPLETE token is "+ str(self.connectionToken))

        pass

    def onInput_inputCompleteConnection(self):
        self.freez = False
        self.logger.info("ACTION_COMPLETE token is "+ str(self.connectionToken))

        pass

    ## ---- START ROBOT ACTIONS ---- ##
    def getTokenMethod(self, connection_token):
        self.freez = True
        self.connectionToken = str(connection_token)
        self.logger.info("GET CONNECTION TOKEN ["+self.connectionToken+"]")

        self.output_get_connection_token(self.connectionToken)
        pass

    def connectionSuccessMethod(self):
        self.freez = True

        self.output_success_connection()
        pass

    def disconnectionMethod(self):
        self.freez = True
        self.output_disconnection()
        pass

    def robotChangeEyecolorMethod(self, eyecolor):
        decoded = urllib.unquote(eyecolor)

        self.logger.info(str(decoded))
        self.logger.info("robot will change eyecolor")
        self.output_change_eyecolor(decoded)
        pass

    def robotMoveRshoulderMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move right shoulder")
        self.output_rshoulder(decoded)
        pass

    def robotMoveLshoulderMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move left shoulder")
        self.output_lshoulder(decoded)
        pass

    def robotMoveRelbowMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move right elbow")
        self.output_relbow(decoded)
        pass

    def robotMoveLelbowMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move left elbow")
        self.output_lelbow(decoded)
        pass

    def robotMoveRwristMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move right wrist")
        self.output_rwrist(decoded)
        pass

    def robotMoveLwristMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move left wrist")
        self.output_lwrist(decoded)
        pass

    def robotMoveHeadMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move head")
        self.output_head(decoded)
        pass

    def robotMoveHipMethod(self, data):
        decoded = urllib.unquote(data)

        self.logger.info(decoded)
        self.logger.info("robot will move hip")
        self.output_hip(decoded)
        pass

    def robotWalkMethod(self):
        self.logger.info("robot will walk")
        self.output_walk()
        pass

    def robotReset(self):
        self.logger.info("robot will reset")
        self.output_reset()
        pass

    def V8(self):
        self.logger.info("Witness ME!!!!")
        self.output_v8()
        pass
    ## ---- END ROBOT ACTIONS ---- ##


    ## ---- START WEBSOCKET FUNCTIONS ---- ##
    def ws_send_message(self, inputJson):
        try:
            jsonData = json.dumps(inputJson)
            self.ws.send(jsonData)
        except ValueError:
            self.logger.info('ws has error')
        pass

    def ws_on_message(self, ws, message):
        self.logger.info('message is ....');
        self.logger.info(message);

        receive = json.loads(message)

        target_action = str(receive["action"])
        target_data   = str(receive["data"])

        if target_action == "get_token" :
            if self.freez == False :
                self.getTokenMethod(target_data)
        elif target_action == "success_connection" :
            self.logger.info("success connection")
            if self.freez == False :
                self.connectionSuccessMethod()
        elif target_action == "change_eyecolor":
            if self.freez == False :
                self.robotChangeEyecolorMethod(target_data)
            pass
        elif target_action == "move_rshoulder":
            if self.freez == False :
                self.robotMoveRshoulderMethod(target_data)
            pass
        elif target_action == "move_lshoulder":
            if self.freez == False :
                self.robotMoveLshoulderMethod(target_data)
            pass
        elif target_action == "move_relbow":
            if self.freez == False :
                self.robotMoveRelbowMethod(target_data)
            pass
        elif target_action == "move_lelbow":
            if self.freez == False :
                self.robotMoveLelbowMethod(target_data)
            pass
        elif target_action == "move_rwrist":
            if self.freez == False :
                self.robotMoveRwristMethod(target_data)
            pass
        elif target_action == "move_lwrist":
            if self.freez == False :
                self.robotMoveLwristMethod(target_data)
            pass
        elif target_action == "move_head":
            if self.freez == False :
                self.robotMoveHeadMethod(target_data)
            pass
        elif target_action == "move_hip":
            if self.freez == False :
                self.robotMoveHipMethod(target_data)
            pass
        elif target_action == "walk":
            if self.freez == False :
                self.robotWalkMethod()
            pass
        elif target_action == "reset":
            if self.freez == False :
                self.robotReset()
            pass
        elif target_action == "V8":
            if self.freez == False :
                self.V8()
            pass
        elif target_action == "disconnection" :
            if self.freez == False :
                self.disconnectionMethod()
            pass
        pass

    def ws_on_error(self, ws, error):
        if self.ws_connection_retry < 3 :
            self.logger.info('reconnect ws')
            time.sleep(1)
            self.ws_connection_retry = self.ws_connection_retry + 1
            self.init_websocket_connection()
        else:
            self.output_connection_error()
        pass

    def ws_on_close(self, ws):
        self.logger.info('connection closed')
        pass

    def ws_on_open(self, ws):
        self.ws_connection_retry = 0
        self.logger.info('connection open')
        pass
    ## ---- END WEBSOCKET FUNCTIONS ---- ##


    ## ---- START INITIALIZE ---- ##
    def init_websocket_connection(self):
        import websocket

        websocket.enableTrace(True)

        websocket.setdefaulttimeout(500)

        websocketUrl = "ws://taptappun.cloudapp.net:3001/"

        self.ws = websocket.WebSocketApp(websocketUrl,
            on_message = self.ws_on_message,
            on_error = self.ws_on_error,
            on_close = self.ws_on_close)

        self.ws.on_open = self.ws_on_open

        self.ws.run_forever()

        pass


    def init_read_library(self):
        self.folderName = os.path.join(self.framemanager.getBehaviorPath(self.behaviorId), "./lib")

        if self.folderName not in sys.path:
            sys.path.append(self.folderName)
    ## ---- END INITIALIZE ---- ##]]>
